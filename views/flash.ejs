<div class="flashes">
    <% if (flashMessages) { %>
        <% if (flashMessages.success) { %>
            <div class="alert alert-success" role="alert"><%= flashMessages.success %></div>
        <% } else if (flashMessages.error) { %>
            <div class="alert alert-danger" role="alert"><%= flashMessages.error %></div>
        <% } %>
    <% } %>
</div>